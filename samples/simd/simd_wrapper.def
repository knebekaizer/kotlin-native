#include <simd/geometry.h>

static simd_int4 myVar4i32 = {1, 4, 9, 25};

float length(simd_float4 f) {
    return simd_length(f);
}

simd_float4 vfadd(simd_float4 v1, simd_float4 v2) {
	return v1 + v2;
}

simd_float4 vfsub(simd_float4 v1, simd_float4 v2) {
	return v1 - v2;
}

